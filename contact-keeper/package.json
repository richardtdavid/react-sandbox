  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "eslint": "^6.2.1",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.6.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  }
}
